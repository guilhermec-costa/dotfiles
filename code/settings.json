{
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.mouseWheelZoom": true,
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "keyboard.dispatch": "keyCode",
    "explorer.confirmDelete": false,
    "vim.cursorStylePerMode.insert": "block",
    "breadcrumbs.enabled": false,
    "vim.cursorStylePerMode.normal": "block",
    "vim.statusBarColorControl": false,
    "vim.leader": "<space>",
    "vim.cursorStylePerMode.replace": "block",
    "vim.cursorStylePerMode.visual": "line-thin",
    "vim.cursorStylePerMode.visualblock": "line-thin",
    "vim.cursorStylePerMode.visualline": "block",
    "editor.minimap.enabled": false,
    "vim.hlsearch": true,
    "zenMode.hideStatusBar": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.centerLayout": false,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "rgba(250, 240, 170, 0.5)",
    "vim.highlightedyank.duration": 50,
    "editor.lineNumbers": "relative",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "<C-w>",
                "k"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<C-w>",
                "j"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<C-w>",
                "l"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "<C-w>",
                "h"
            ]
        },
        {
            "before": [
                "<C-d>"
            ],
            "after": [
                "<C-d>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-u>"
            ],
            "after": [
                "<C-u>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<leader>",
                "d",
                "p"
            ],
            "commands": [
                "editor.action.marker.prev"
            ]
        },
        {
            "before": [
                "<leader>",
                "d",
                "n"
            ],
            "commands": [
                "editor.action.marker.next"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "l"
            ],
            "commands": [
                "workbench.action.splitEditorRight"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "j"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        },
        {
            "before": [
                "<leader>",
                "p",
                "v"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "l"
            ],
            "commands": [
                "editor.action.clearHighlight"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "l"
            ],
            "commands": [
                ":nohlsearch"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "commands": [
                "workbench.action.reloadWindow"
            ]
        },
        {
            "before": [
                "<leader>",
                "kp"
            ],
            "after": [
                "\"",
                "k",
                "p"
            ]
        },
        {
            "before": [
                "<leader>",
                "sp"
            ],
            "commands": [
                "editor.action.clipboardPasteAction"
            ]
        },
        {
            "before": [
                "<leader>",
                "sy"
            ],
            "commands": [
                "editor.action.clipboardCopyAction"
            ]
        },
        {
            "before": [
                "<C-Up>"
            ],
            "commands": [
                "workbench.action.increaseViewSize"
            ]
        },
        {
            "before": [
                "<C-Down>"
            ],
            "commands": [
                "workbench.action.decreaseViewSize"
            ]
        },
        {
            "before": [
                "<C-Right>"
            ],
            "commands": [
                "workbench.action.increaseViewSize"
            ]
        },
        {
            "before": [
                "<C-Left>"
            ],
            "commands": [
                "workbench.action.decreaseViewSize"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "<leader>",
                "F"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "<C-m>"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "1"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex1"
            ]
        },
        {
            "before": [
                "<leader>",
                "2"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex2"
            ]
        },
        {
            "before": [
                "<leader>",
                "3"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex3"
            ]
        },
        {
            "before": [
                "<leader>",
                "4"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex4"
            ]
        },
        {
            "before": [
                "<leader>",
                "5"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex5"
            ]
        },
        {
            "before": [
                "<leader>",
                "6"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex6"
            ]
        },
        {
            "before": [
                "<leader>",
                "7"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex7"
            ]
        },
        {
            "before": [
                "<leader>",
                "8"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex8"
            ]
        },
        {
            "before": [
                "<leader>",
                "9"
            ],
            "commands": [
                "workbench.action.openEditorAtIndex9"
            ]
        },
        {
            "before": [
                "<leader>",
                "0"
            ],
            "commands": [
                "workbench.action.lastEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "Q"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "g",
                "c",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "<S-k>"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "<leader>",
                "C"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "s"
            ],
            "commands": [
                "workbench.action.focusSideBar"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-j>"
            ],
            "commands": [
                "editor.action.moveLinesDownAction"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "commands": [
                "editor.action.moveLinesUpAction"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "leader",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        }
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-j>"
            ],
            "commands": [
                "selectNextSuggestion"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "commands": [
                "selectPrevSuggestion"
            ]
        },
    ],
    "editor.fontFamily": "Iosevka Fixed, Fira Code, Cascadia Mono, Consolas, 'Courier New', monospace, JetBrains Mono NL SemiBold",
    "terminal.integrated.fontFamily": "JetBrains Mono NL SemiBold",
    "C_Cpp.default.cppStandard": "gnu++17",
    "workbench.preferredDarkColorTheme": "Default Dark+",
    "workbench.colorTheme": "Kanagawa Dragon",
    "security.workspace.trust.untrustedFiles": "open",
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "editor.fontLigatures": true,
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.validate.enable": true,
    "window.customTitleBarVisibility": "auto",
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    "files.associations": {
        "*.js": "javascript"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.menuBarVisibility": "compact",
    "window.commandCenter": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "database-client.autoSync": true
}