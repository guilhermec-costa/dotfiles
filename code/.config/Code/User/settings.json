{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.mouseWheelZoom": true,
  "window.titleBarStyle": "custom",
  "window.customMenuBarAltFocus": false,
  "terminal.integrated.altClickMovesCursor": false,
  "window.enableMenuBarMnemonics": false,
  "window.zoomLevel": 0,
  "keyboard.dispatch": "keyCode",
  "explorer.confirmDelete": false,
  "workbench.editor.enablePreview": false,
  "vim.cursorStylePerMode.insert": "block",
  "vim.cursorStylePerMode.normal": "block",
  "vim.statusBarColorControl": false,
  "vim.leader": "<space>",
  "vim.cursorStylePerMode.replace": "block",
  "vim.cursorStylePerMode.visual": "line-thin",
  "vim.cursorStylePerMode.visualblock": "line-thin",
  "vim.cursorStylePerMode.visualline": "block",
  "search.quickOpen.includeHistory": false,
  "search.quickOpen.includeSymbols": false,
  "editor.minimap.enabled": false,
  "vim.hlsearch": true,
  "zenMode.hideStatusBar": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(250, 240, 170, 0.5)",
  "vim.highlightedyank.duration": 50,
  "editor.lineNumbers": "relative",
  "workbench.tree.indent": 16,
  "terminal.integrated.cursorBlinking": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": ["<C-k>"], "after": ["<C-w>", "k"] },
    { "before": ["<C-j>"], "after": ["<C-w>", "j"] },
    { "before": ["<C-l>"], "after": ["<C-w>", "l"] },
    { "before": ["<C-h>"], "after": ["<C-w>", "h"] },
    { "before": ["<C-d>"], "after": ["<C-d>", "z", "z"] },
    { "before": ["<C-u>"], "after": ["<C-u>", "z", "z"] },
    { "before": ["<leader>", "c", "s"],
      "commands": ["workbench.action.selectTheme"]
    },
    { "before": ["<leader>", "g", "b"],
      "commands": ["gitblame.gitShow"]
    },
    {
      "before": ["<leader>", "d", "p"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["<leader>", "d", "n"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "s", "l"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["<leader>", "s", "j"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["<leader>", "p", "v"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "c", "l"],
      "commands": ["editor.action.clearHighlight"]
    },
    { "before": ["<leader>", "c", "l"], "commands": [":nohlsearch"] },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.reloadWindow"]
    },
    { "before": ["<leader>", "kp"], "after": ["\"", "k", "p"] },
    {
      "before": ["<leader>", "sp"],
      "commands": ["editor.action.clipboardPasteAction"]
    },
    {
      "before": ["<leader>", "sy"],
      "commands": ["editor.action.clipboardCopyAction"]
    },
    { "before": ["<C-Up>"], "commands": ["workbench.action.increaseViewSize"] },
    {
      "before": ["<C-Down>"],
      "commands": ["workbench.action.decreaseViewSize"]
    },
    {
      "before": ["<C-Right>"],
      "commands": ["workbench.action.increaseViewSize"]
    },
    {
      "before": ["<C-Left>"],
      "commands": ["workbench.action.decreaseViewSize"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "F"],
      "commands": ["editor.action.formatDocument"]
    },
    { "before": ["<C-n>"], "commands": ["workbench.action.previousEditor"] },
    { "before": ["<C-m>"], "commands": ["workbench.action.nextEditor"] },
    {
      "before": ["<leader>", "1"],
      "commands": ["workbench.action.openEditorAtIndex1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["workbench.action.openEditorAtIndex2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["workbench.action.openEditorAtIndex3"]
    },
    {
      "before": ["<leader>", "4"],
      "commands": ["workbench.action.openEditorAtIndex4"]
    },
    {
      "before": ["<leader>", "5"],
      "commands": ["workbench.action.openEditorAtIndex5"]
    },
    {
      "before": ["<leader>", "6"],
      "commands": ["workbench.action.openEditorAtIndex6"]
    },
    {
      "before": ["<leader>", "7"],
      "commands": ["workbench.action.openEditorAtIndex7"]
    },
    {
      "before": ["<leader>", "8"],
      "commands": ["workbench.action.openEditorAtIndex8"]
    },
    {
      "before": ["<leader>", "9"],
      "commands": ["workbench.action.openEditorAtIndex9"]
    },
    {
      "before": ["<leader>", "0"],
      "commands": ["workbench.action.lastEditor"]
    },
    {
      "before": ["<leader>", "Q"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    { "before": ["g", "c", "c"], "commands": ["editor.action.commentLine"] },
    { "before": ["<S-k>"], "commands": ["editor.action.showHover"] },
    {
      "before": ["<leader>", "C"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "d", "d"],
      "commands": ["workbench.actions.view.problems"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "o", "i"],
      "commands": ["editor.action.organizeImports"]
    },
    {
      "before": ["<leader>", "f", "g"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<leader>", "f", "b"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "l", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["workbench.view.scm"]
    },
    { 
      "before": ["<leader>", "M"],
      "commands": [":marks"],
    },
    {
      "before": ["<leader>", "l", "s"],
      "commands": ["git.stashView"]
    },
    {
      "before": ["<leader>", "="],
      "commands": ["editor.action.fontZoomIn"]
    },
    {
      "before": ["<leader>", "-"],
      "commands": ["editor.action.fontZoomOut"]
    },
    {
      "before": ["<leader>", "0"],
      "commands": ["workbench.action.zoomReset"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    { "before": ["<C-j>"], "commands": ["editor.action.moveLinesDownAction"] },
    { "before": ["<C-k>"], "commands": ["editor.action.moveLinesUpAction"] },
    { "before": ["<"], "commands": ["editor.action.outdentLines"] },
    { "before": [">"], "commands": ["editor.action.indentLines"] },
    { "before": ["leader", "c"], "commands": ["editor.action.commentLine"] },
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    { "before": ["<C-j>"], "commands": ["selectNextSuggestion"] },
    { "before": ["<C-k>"], "commands": ["selectPrevSuggestion"] },
    { "before": ["j", "j"], "after": ["<Esc>"] },
    { "before": ["<CapsLock>"], "after": ["<Esc>"] }
  ],
  "editor.fontFamily": "Iosevka Fixed, Fira Code Regular, Cascadia Mono, Consolas, 'Courier New', monospace, JetBrains Mono NL SemiBold",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "terminal.integrated.fontFamily": "JetBrains Mono NL SemiBold",
  "C_Cpp.default.cppStandard": "gnu++17",
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "security.workspace.trust.untrustedFiles": "open",
  "files.exclude": {
    "**/node_modules": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.venv/**": true,
    "**/__pycache__/**": true,
    "**/*.pyc": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.next/**": true,
    "**/coverage/**": true,
    "**/logs/**": true,
    "**/*.gz": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.venv": true,
    "**/__pycache__": true,
    "**/dist": true,
    "**/build": true,
    "**/.next": true,
    "**/coverage": true
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java",
  },
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
"javascript.validate.enable": true,
  "window.customTitleBarVisibility": "auto",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "files.associations": {
    "*.js": "javascript"
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
},
"window.menuBarVisibility": "compact",
"window.commandCenter": false,
"[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
},
"database-client.autoSync": true,
"dotnet.codeLens.enableReferencesCodeLens": false,
"editor.suggestOnTriggerCharacters": false,
"editor.suggest.selectionMode": "never",
"editor.inlineSuggest.enabled": false,
"editor.quickSuggestions": {
  "comments": "on",
  "other": "off",
  "strings": "off"
},
"editor.suggest.showWords": false,
"editor.suggest.showSnippets": true,
"editor.inlayHints.enabled": "offUnlessPressed",
"C_Cpp.intelliSenseEngine": "disabled",
"javascript.updateImportsOnFileMove.enabled": "always",
"[json]": {
      "editor.defaultFormatter": "biomejs.biome"
},
"workbench.colorTheme": "Night Owl (No Italics)",
"editor.defaultFormatter": "biomejs.biome",
"debug.disassemblyView.showSourceCode": false
}